{
  "name": "ru-bot",
  "version": "0.1.0",
  "description": "Telegram bot to manage spam and rules for @_ru community",
  "main": "src/index.js",
  "scripts": {
    "test": "eslint .",
    "dev": "DEBUG=rubot:* nodemon -e yaml,js,json -w locales -w src ./src",
    "start": "pm2 startOrRestart ./process.config.js",
    "migrate": "sequelize db:migrate",
    "rollback": "sequelize db:migrate:undo",
    "prepush": "npm test",
    "precommit": "npm test --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LestaD/ru_bot.git"
  },
  "keywords": [
    "telegram",
    "bot"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "author": "Sergey Sova <i.am@lestad.net> (https://lestad.top)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LestaD/ru_bot/issues"
  },
  "homepage": "https://github.com/LestaD/ru_bot#readme",
  "devDependencies": {
    "eslint": "^4.11.0",
    "eslint-config-atomix-base": "^4.6.0",
    "husky": "^0.14.3",
    "nodemon": "^1.12.1",
    "pm2": "^2.7.2",
    "sequelize-cli": "^3.0.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.6",
    "telegraf": "^3.15.3",
    "telegraf-botanio": "^0.5.1"
  }
}
